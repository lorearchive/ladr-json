      "BGName": 0,
      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "3;모모이;07;우타하 선배…….\n#3;em;…",
      "TextJp": "ウタハ先輩……。",
      "TextTh": "รุ่นพี่อุทาฮะ...",
      "TextTw": "詠葉前輩……",
      "TextEn": "Utaha...",
      "VoiceId": 0,
      "TeenMode": false
    },
