            "BGMId": 0,
            "Sound": "",
            "Transition": 0,
            "BGName": 0,
            "BGEffect": 0,
            "PopupFileName": "",
            "ScriptKr": "5;\uc138\ub9ac\uce74 \ubd80\uc0c1;03;\uc6b0\uc640! \ub3c4\uc6b0\ub7ec \uc640\uc92c\uad6c\ub098!! \n#5;a\n#5;em;[\ubc18\uc751]\n#5;hophop",
            "TextJp": "\u52a9\u3051\u306b\u304d\u3066\u304f\u308c\u305f\u306e\u306d\uff01",
            "TextTh": "\u0e27\u0e49\u0e32\u0e27! \u0e21\u0e32\u0e0a\u0e48\u0e27\u0e22\u0e01\u0e31\u0e19\u0e2a\u0e34\u0e19\u0e30!! ",
            "TextTw": "\u54c7\uff01\u4f60\u4f86\u5e6b\u6211\u5011\u4e86\u55ce\uff01\uff01",
