            "ScriptKr": "5;\ud504\ub77c\ub098;05;\ud558\uc9c0\ub9cc \uc774\uac83\uc73c\ub85c\u2026\u2026.\n#5;em;[!]",
            "TextJp": "\u3067\u3059\u304c\u3001\u3053\u308c\u3067\u2026\u2026\u3002",
            "TextTh": "\u0e41\u0e15\u0e48\u0e27\u0e48\u0e32\u0e19\u0e35\u0e48\u0e08\u0e30\u0e17\u0e33\u0e43\u0e2b\u0e49...",
