            "BGEffect": 0,
            "PopupFileName": "",
            "ScriptKr": "1;\ub178\ub178\ubbf8 \ubd80\uc0c1;04;\ub2e4, \ub2e4\ub978 \uc138\uacc4\uc120\uc758 \uc2dc\ub85c\ucf54\u2026\u2026\uca29\u2026\u2026?\n#1;a",
            "TextJp": "\u3082\u3046\u4e00\u4eba\u306e\u3001\u30b7\u30ed\u30b3\u3061\u3083\u3093\u2026\u2026\uff1f",
            "TextTh": "\u0e0a\u0e30... \u0e0a\u0e34\u0e42\u0e23\u0e42\u0e01\u0e30\u0e08\u0e31\u0e07... \u0e08\u0e32\u0e01\u0e42\u0e25\u0e01\u0e2d\u0e37\u0e48\u0e19\u0e40\u0e2b\u0e23\u0e2d...?",
            "TextTw": "\u662f\u5176\u4ed6\u6642\u9593\u8ef8\u7684\u767d\u5b50\u2026\u2026\u55ce\uff1f",
            "TextEn": "The other world's...Shiroko?",
            "VoiceId": 0,
            "TeenMode": false
        },
