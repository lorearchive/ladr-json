            "ScriptKr": "3;\uc2dc\ub85c\ucf54 \ud14c\ub7ec \uc5b8\ub178\uc6b4;04;\uc774\uac74\u2026\u2026 \uc124\ub9c8 \ub098\ub97c\u2026\u2026?",
            "TextJp": "\u2026\u2026\u79c1\u3092\u8ffd\u3063\u3066\u304d\u305f\u3093\u3060\u3002",
            "TextTh": "\u0e21\u0e31\u0e19\u0e15\u0e32\u0e21... \u0e09\u0e31\u0e19\u0e21\u0e32...?",
            "TextTw": "\u9019\u2026\u2026\u8a72\u4e0d\u6703\u662f\u2026\u2026\uff1f",
