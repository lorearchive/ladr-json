            "ScriptKr": "3;\ud1b5\uc2e0\uc544\ub85c\ub098;03;\uc88b\uc740 \uc544\uce68\uc774\uc5d0\uc694, \uc120\uc0dd\ub2d8!\n#3;greeting\n#3;a\n#3;em;[\ubc18\uc9dd]",
            "TextJp": "\u304a\u306f\u3088\u3046\u3054\u3056\u3044\u307e\u3059\u3001\u5148\u751f\uff01",
            "TextTh": "\u0e2d\u0e23\u0e38\u0e13\u0e2a\u0e27\u0e31\u0e2a\u0e14\u0e34\u0e4c\u0e04\u0e48\u0e30\u0e04\u0e38\u0e13\u0e04\u0e23\u0e39!",
            "TextTw": "\u65e9\u5b89\uff0c\u8001\u5e2b\uff01",
