            "BGEffect": 0,
            "PopupFileName": "",
            "ScriptKr": "3;\ud1b5\uc2e0\uc544\ub85c\ub098;01;\uc774\uac74 \uc120\uc0dd\ub2d8\uc774 \ud55c\ubc88 \uc77d\uc5b4\ubcf4\uc154\uc57c \ud560 \uac70 \uac19\uc544\uc11c\uc694.",
            "TextJp": "\u3053\u308c\u306f\u5148\u751f\u306b\u4e00\u5ea6\u8aad\u3093\u3067\u3082\u3089\u3063\u305f\u65b9\u304c\u826f\u3044\u304b\u306a\u3068\u3002",
            "TextTh": "\u0e2b\u0e19\u0e39\u0e27\u0e48\u0e32\u0e04\u0e38\u0e13\u0e04\u0e23\u0e39\u0e04\u0e27\u0e23\u0e25\u0e2d\u0e07\u0e2d\u0e48\u0e32\u0e19\u0e14\u0e39\u0e19\u0e30\u0e04\u0e30",
            "TextTw": "\u9019\u8acb\u8001\u5e2b\u5148\u8b80\u904e\u4e00\u904d\u6bd4\u8f03\u597d\u3002",
            "TextEn": "I think you should take a look.",
            "VoiceId": 0,
            "TeenMode": false
