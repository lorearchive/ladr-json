            "ScriptKr": "5;\uc138\ub9ac\uce74;12;\uad1c\ucc2e\uc544!\n#5;a\n#5;em;[\ubc18\uc751]",
            "TextJp": "\u5168\u3063\u7136\u5927\u4e08\u592b\uff01",
            "TextTh": "\u0e44\u0e21\u0e48\u0e40\u0e1b\u0e47\u0e19\u0e44\u0e23!\n\n",
            "TextTw": "\u5b8c\u5168\u6c92\u554f\u984c\uff01",
