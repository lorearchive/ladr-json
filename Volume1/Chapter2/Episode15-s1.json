      "Transition": 0,
      "BGName": 0,
      "BGEffect": 0,
      "PopupFileName": "",
      "ScriptKr": "3;카이저 PMC 이사;01;!!\n#3;stiff",
      "TextJp": "！！",
      "TextTh": "!!\n",
      "TextTw": "！！",
      "TextEn": "...!",
